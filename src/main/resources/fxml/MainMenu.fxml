   <children>
      <Label layoutX="231.0" layoutY="30.0" text="Store Manager">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Button fx:id="statisticsButton" layoutX="254.0" layoutY="116.0" mnemonicParsing="false" onAction="#showStatisticsMenu" text="Statistics" />
      <Button fx:id="searchButton" layoutX="259.0" layoutY="91.0" mnemonicParsing="false" onAction="#showSearchMenu" text="Search" />
      <Button fx:id="exitButton" layoutX="274.0" layoutY="332.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
   </children>
</AnchorPane>
